{
  "name": "moleculer-sentry",
  "version": "0.0.3",
  "description": "Moleculer service for sentry error monitoring",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YourSoftRun/moleculer-sentry.git"
  },
  "keywords": [
    "moleculer",
    "sentry"
  ],
  "author": "Hugo Meyronneinc <hugo@yoursoft.run>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YourSoftRun/moleculer-sentry/issues"
  },
  "homepage": "https://github.com/YourSoftRun/moleculer-sentry#readme",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "dependencies": {
    "@sentry/node": "^5.0.5"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "moleculer": "^0.13.8"
  }
}
